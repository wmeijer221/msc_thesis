    {
      "id": 78787669,
      "type": null,
      "body": "Oh, one more thing: that regex for the email address scares the bejesus out of me! Maybe you could add a comment pointing to the source/authority for it (RFC, StackOverflow answer, blog post, whatever) just in case we ever need to look at it for some reason.\n\nCheers.\n",
      "attachment": null,
      "author": {
        "id": 374886,
        "username": "philbooth",
        "name": "Phil Booth",
        "state": "active",
        "avatar_url": "https://secure.gravatar.com/avatar/6b280328ebb187f206ac98c07c8f31db?s=80&d=identicon",
        "web_url": "https://gitlab.com/philbooth"
      },
      "created_at": "2014-04-23T08:10:51.000Z",
      "updated_at": "2014-04-23T08:10:51.000Z",
      "system": false,
      "noteable_id": 12069259,
      "noteable_type": "MergeRequest",
      "project_id": 6666488,
      "resolvable": false,
      "confidential": false,
      "internal": false,
      "noteable_iid": 20,
      "commands_changes": {},
      "award_emoji_data": []
